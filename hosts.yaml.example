all:
  children:
    zookeeper:
      hosts:
        zk1.internal:
        zk2.internal:
    mysql:
      hosts:
        mysql1.internal:
    gerrit:
      hosts:
        gerrit.internal:
      vars:
        remote_user: ubuntu
    nexus:
      hosts:
        nexus.internal:
      vars:
        remote_user: ubuntu
    zuul-scheduler:
      hosts:
        zuul.internal:
      vars:
        remote_user: ubuntu
        zuul_front_port: 8080
        zuul_logs_port: 8082
    zuul-executor:
      hosts:
        zuul1.internal:
        zuul2.internal:
        zuul3.internal:
    zuul-web:
      hosts:
        zuul-web1.internal:
        zuul-web2.internal:
    nodepool-builder:
      hosts:
        nodepool-builder.internal:
    nodepool-launcher:
      hosts:
        nodepool-launcher.internal:
  vars:
    zuul_build_with_docker: false
    zuul_config_project: "Juniper/contrail-project-config"
    node_docker_port: 2375
    nexus_host: "nexus.internal"
    zuul_log_url: "http://log_site.public:port"
    zookeeper_host: z11.internal
    mysql_host: mysql1.internal
    mysql_root_password: rootsecret
    zuul_mysql_username: zuul
    zuul_mysql_password: secret
    zuul_mysql_db_main: zuul
    zuul_mysql_db_build_number: zuul_buildnumber
    gerrit_front_port: 80
    gerrit_host: "internal.gerrit.name"
    gerrit_url: "http://gerrit.public:{{ gerrit_front_port }}"
    gerrit_ssh_keys: "{{ lookup('env', 'HOME') }}/gerrit/ssh"
    gerrit_force_ssh_keys_generate: false
    gerrit_force_create_zuul_user: false
    gerrit_source_url: "https://review.opencontrail.org"
    force_update_repos: true
    gerrit_repos:
      - {dest_namespace: "Juniper", project: "contrail"}
      - {dest_namespace: "Juniper", project: "contrail-analytics"}
      - {dest_namespace: "Juniper", project: "contrail-ansible"}
